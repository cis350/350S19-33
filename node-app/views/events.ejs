<!DOCTYPE html>
<html>
<h1> Events </h1>
<body>
<table class="table">
  <thead class="thead-dark">
    <tr>
      <th scope="col">Name</th>
      <th scope="col">Location</th>
      <th scope="col">Time</th>
      <th scope="col">Date</th>
    </tr>
  </thead>
  <tbody>
    <% events.forEach((event) => { %>
      <tr>
        <th> <%= event.name %> </th>
        <td> <%= event.location %> </td>
        <td> <%= event.time %> </td>
        <td> <%= event.date %> </td>
        <td> <a href='/event?id=<%=event.id%>'> Show </a> </td>
      </tr>
    <% }); %>
  </tbody>
</table>
</body>
</html>
